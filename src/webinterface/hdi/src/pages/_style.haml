    /* General styles */
    html, body
      color: #000
      font: normal 90% Arial, Helvetica, sans-serif
      height: 100%
      min-width: 980px
      margin: 0
      padding: 0
      overflow: hidden
    *:link, *:visited
      color: #ba1e16
      text-decoration: none
    *:link img, *:visited img
      border: none
    h1
      background: #333
      border-right: 2px solid #111
      border-bottom: 1px solid #333
      color: #999
      font: normal 125% Arial, Helvetica, sans-serif
      height: 32px
      line-height: 32px
      margin: 0
      padding: 0 0 0 .5em
      position: relative
    h1 :link, h1 :visited, h1 strong
      padding: .4em .5em
    h1 :link, h1 :visited
      background: url(../image/path.gif) 100% 50% no-repeat
      color: #bbb
      cursor: pointer
      padding-right: 2.2em
      text-shadow: #333 2px 2px 1px
    h1 strong
      color: #fff
      font-weight: normal
      padding-right: 25px
    h1 strong a
      color: #fff !important
      background: none !important
    h1 :link.raw, h1 :visited.raw
      background: 100% 50% no-repeat
      position: absolute
      right: 20px
      width: 35px
      height: 100%
      padding: 0
      margin: 0
    hr
      border: 1px solid #999
      border-width: 1px 0 0
    dl dt
      font-weight: bold
    code, tt, pre
      font-family: "DejaVu Sans Mono", Menlo, Monaco, Courier, monospace
      font-size: small
    code.key
      color: #333
      font-weight: bold
    code.string
      color: #fff
      background: black
    /* Tabular listings */
    #branches_column
      width: 20%
    #checkouts_column
      width: 30%
    #output_column
      width: 50%
    #pre_output
      overflow: auto
    table.listing
      border-collapse: separate
      border-spacing: 0
      border: 1px solid #a7a7a7
      clear: both
      width: 100%
      caption
        display: none
      th
        padding: .2em .5em
      thead th
        background: #dadada repeat-x
        border: 1px solid #a7a7a7
        border-width: 0 0 1px 1px
        color: #333
        font-size: 95%
        font-weight: normal
        text-align: left
        text-shadow: #999 2px 1px 2px
        white-space: nowrap
        &:first-child
          border-left: none
        &.key
          background: #a7afb6 0 0 repeat-x
          padding-top: 2px
          span
            background: 100% 3px no-repeat
            cursor: pointer
        &.desc span
    .data
      width: 100%
    .data, .data tr
      margin: 0
      padding: 0
      border: 0
    .data tr td
      padding: .75em 1em
      cursor: pointer
      color: #4b6e03
    .data tbody tr.selected td
      background-color: #6b8e23
      color: white
    time
      font-size: x-small
    .data_column
      overflow: auto
    table.listing tbody tr th, table.listing tbody tr td
      background: #feffea
    table.listing tbody tr.odd th, table.listing tbody tr.odd td,
    table.listing tbody.odd tr th, table.listing tbody.odd tr td
      background: #fff
    table.listing tbody th
      border-left: 1px solid #d9d9d9
      padding: .4em .5em
      vertical-align: top
    table.listing tbody td
      vertical-align: top
      border-left: 1px solid #a7a7a7
    table.listing tbody th:first-child, table.listing tbody td:first-child
      border-left: none
    table.listing tbody th
      text-align: left
    table.listing tbody th :link, table.listing tbody th :visited
      display: block
    #wrap
      background: #fff 100% 0 repeat-y
      height: 100%
      margin-right: 210px
      position: relative
    body.fullwidth #wrap
      margin-right: 0
    #content
      margin-top: 64px
      padding: 0 20px
      overflow: auto
      overflow-y: scroll
    #deployments
      margin: 20px 0
    /* FANCY MILITARY HEADER */
    body
      background: #ddd
      background: -webkit-gradient(linear,left bottom,left top,color-stop(0, rgb(153,153,153)),color-stop(1, rgb(245,245,245)))
      background: -moz-linear-gradient(center bottom,rgb(153,153,153)0%, rgb(245,245,245) 100%)
    header, footer, aside
      display: block
    header
      z-index: 1000
      height: 64px
      background: #444 url('data:image/gif;base64,') repeat scroll top left
      text-align: center
      padding: 0
      border-bottom: 1px solid #444
      border-top: 3px solid black
      -webkit-box-shadow: #000 0 0 10px
      -moz-box-shadow: #000 0 0 10px
      position: fixed
      width: 100%
      min-width: 980px
    header title
      margin: 0 auto
      width: 400px
      clear: both
    header h1, header h2
      margin: 0
      padding: 0
      color: white
      text-shadow: #000 0 2px 0
      line-height: 1em
    header h1
      font-size: 18px
      margin: 0
      padding: 0
    header h2
      font-size: 16px
      font-family: helvetica, arial, sans, sans-serif
      font-weight: 900
      text-transform: lowercase
      font-variant: small-caps
      color: #6B8E23
    header .on_github a
      position: absolute
      right: 18px
      top: 10px
      background: #80858a
      -webkit-border-radius: 6px
      -moz-border-radius: 6px
      padding: 8px 22px
      line-height: 1em
      font-size: 12px
      color: #fff
      cursor: pointer
      -webkit-box-shadow: #000 0 1px 1px inset, #bbb 0 1px 1px
      -moz-box-shadow: #000 0 1px 1px inset, #bbb 0 1px 1px
      text-shadow: #555 0 0 4px
    header .on_github a:hover
      background: #90959a
    header .on_github a:active
      background: #70757a
      top: 11px
    header .project_info
      -webkit-box-shadow: #000 0 1px 1px inset, #bbb 0 1px 1px
      -moz-box-shadow: #000 0 1px 1px inset, #bbb 0 1px 1px
      background: #333
      padding: 0px 20px
      font-size: 20px
      color: white
      position: absolute
      left: 18px
      top: 12px
      -webkit-border-radius: 1em
      -moz-border-radius: 1em
      line-height: 30px
      vertical-align: middle
    header .project_info a, header .on_github a
      color: white
      text-decoration: none
    header .project_info a:hover, header .on_github:hover a
      color: #ADF
      text-decoration: none
    .clear
      clear: both
